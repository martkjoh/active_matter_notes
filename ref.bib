	eprint = {https://www.pnas.org/content/117/18/9706.full.pdf},
	issn = {0027-8424},
	journal = {Proceedings of the National Academy of Sciences},
	number = {18},
	pages = {9706--9711},
	publisher = {National Academy of Sciences},
	title = {Emergence of self-organized multivortex states in flocks of active rollers},
	url = {https://www.pnas.org/content/117/18/9706},
	volume = {117},
	year = {2020},
	Bdsk-Url-1 = {https://www.pnas.org/content/117/18/9706},
	Bdsk-Url-2 = {https://doi.org/10.1073/pnas.2000061117}}

