	pages = {{726-737}},
	researcherid-numbers = {{Hemelrijk, Charlotte/E-4828-2013}},
	title = {{Schools of fish and flocks of birds: their shape and internal structure by self-organization}},
	unique-id = {{ISI:000310522300005}},
